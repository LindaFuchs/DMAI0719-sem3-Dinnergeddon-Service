<Window x:Class="DinnergeddonUI.Dashboard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DinnergeddonUI"
        mc:Ignorable="d"
        Title="Dashboard" Height="450" Width="800"
        Background="{StaticResource BackgroundColor}">
    <StackPanel>
        <WrapPanel HorizontalAlignment="Right">
            <Button 
            Style="{StaticResource RoundButtonTemplate}"
            Background="{StaticResource ButtonColor}"
            Foreground="White"
            Height="50"
            Width="90"
            Content="Create lobby" Click="Button_Click"/>
        </WrapPanel>
        <Button Content="Back" HorizontalAlignment="Left" Height="29" Margin="32,0,0,0" Width="75" Click="Button_Click_1"/>
    </StackPanel>
</Window>
